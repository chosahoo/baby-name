export const searchNameByKeyword = (keyword) => {
  const allNames = [...nameStatistics.girl, ...nameStatistics.boy]
  return allNames.filter(nameData =>
    nameData.name.includes(keyword) ||
    nameData.meaning.includes(keyword)
  )
}
